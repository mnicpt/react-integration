{
  "name": "react-integration",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@paypal/messaging-components": "^1.12.1",
    "@paypal/paypal-js": "^1.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "braintree-web-drop-in": "^1.25.0",
    "graphql-resolvers": "^0.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "setup": "npm install && npm run flow-typed",
    "build": "npm run test && npm run babel && npm run webpack && npm run flow:build",
    "start": "react-scripts start",
    "flow-typed": "flow-typed install",
    "flow": "flow",
    "flow:build": "flow gen-flow-files ./src/index.js --out-dir ./dist/module",
    "babel": "babel src/ --out-dir dist/module",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "webpack": "babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/webpack --progress"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/preset-flow": "^7.10.4",
    "flow-bin": "^0.130.0",
    "grumbler-scripts": "^6.0.0",
    "react-router-dom": "^5.2.0"
  }
}
